<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorDivider"/>
            <corners android:radius="@dimen/dimen_loading_rectangle_corner"/>
            <stroke android:color="@color/colorSecondaryVariant" android:width="@dimen/dimen_divider_height"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/colorPrimary"/>
            <corners android:radius="@dimen/dimen_loading_rectangle_corner"/>
            <stroke android:color="@color/colorSecondary" android:width="@dimen/dimen_divider_height"/>
        </shape>
    </item>
</selector>